            function forgot_close_button() {
                $('#ctl00_txtforgotemail').val('');
                $('#ctl00_Label1').hide();
                document.getElementById('ctl00_btnforgot').disabled = '';
                document.getElementById("ctl00_txtforgotemail").disabled = '';
