                                <asp:Label ID="lblinvitemsg" runat="server" ForeColor="Green"></asp:Label>
                            </ContentTemplate>
                        </asp:UpdatePanel>
                    </div>
                </div>

            </div>
        </div>
           <div>


            <div class="chat-sidebar" id="All_online">
